{
    "extends":["rallycoding","plugin:react/recommended"],
    "plugins":[
        "react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true,
        "shared-node-browser": true
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
        "global-require":"off"
    }
}